{
    "name": "nest-microservices",
    "version": "1.0.0",
  "scripts": {
    "start": "npm-run-all --parallel start:main start:product start:users",
    "start:main": "cd main && npm run start:dev",
    "start:listener": "cd main && npm run listener",
    "start:product": "cd product && npm run start:dev",
    "start:users": "cd users && npm run start:dev"
  },
    "devDependencies": {
        "npm-run-all": "^4.1.5"
    }
}
